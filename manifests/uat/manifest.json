{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.3/MicrosoftTeams.schema.json",
    "manifestVersion": "1.3",
    "version": "0.6.0",
    "id": "02d4ddb6-6251-4c04-90ca-21db8d4e78ab",
    "packageName": "com.luckydrawbot.uat",
    "developer": {
        "name": "Tony",
        "websiteUrl": "https://www.microsoft.com",
        "privacyUrl": "https://www.microsoft.com",
        "termsOfUseUrl": "https://www.microsoft.com"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "LuckyDraw",
        "full": "Lucky-Draw Bot (uat)"
    },
    "description": {
        "short": "Lucky Draw Bot",
        "full": "A smart bot(uat) for lucky draw activities in channels."
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "b41371d0-b7fb-4fb1-b182-943c1b051c48",
            "scopes": [
                "team",
                "personal"
            ],
            "commandLists": [
                {
                    "scopes": [
                        "team"
                    ],
                    "commands": [
                        {
                            "title": "help",
                            "description": "Get quick answers and advanced tips"
                        },
                        {
                            "title": "Prize, # of prizes, Draw Time",
                            "description": "Example: Surface Pro, 6, 23:59"
                        },
                        {
                            "title": "Prize, # of prizes, Time, Image",
                            "description": "Example: Surface Pro, 6, 2019-08-08 00:00, http://www.a.com/b.jpg"
                        }
                    ]
                }
            ],
            "supportsFiles": true,
            "isNotificationOnly": false
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "*.ngrok.io"
    ]
}